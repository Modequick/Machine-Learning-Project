    "papermill": {
     "duration": 0.113722,
     "end_time": "2023-05-15T15:33:49.343451",
     "exception": false,
     "start_time": "2023-05-15T15:33:49.229729",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "\n",
    "def build_min_max_scaler(df) :\n",
    "  scaled_columns = pd.DataFrame()\n",
    "  for i in df.columns:\n",
    "      min = df[i].min()\n",
    "      df[i] = df[i]-min\n",
    "      max = df[i].max()\n",
    "      scaled_columns[i] = pd.DataFrame( round(df[i]/max ,5) )\n",
    "  return scaled_columns\n",
    "\n",
    "\n",
    "df_new = build_min_max_scaler(df.drop(\"price\",axis = 1 ))\n",
    "df = df_new.join(df[\"price\"])"
   ]
  },
